SEQUENCE,
HERO_MESSAGE,
MESSAGE2,
MESSAGE3,
MESSAGE4,
MESSAGE5,
MESSAGE6,
PLUS_POINTS,
REWARDS_DATE,
TC,
DATA_DATE
from UNICA_TEMP.DE_FM_TARGETING_DAILY_MC
where ch_em_yn='Y'
;

