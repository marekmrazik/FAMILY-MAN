SET EXCLUDE_REASON_CD =  10
WHERE 	EXCLUDE_REASON_CD IS NULL AND
			EXISTS	(
						SELECT *
						FROM PRS_SECURE_V.MDM_USER_PII AS PII
						WHERE
							A.DEMO_USER_ID = PII.USER_ID AND
							PII.CNFRMD_YN = 'N'
						)
;

--GXO
