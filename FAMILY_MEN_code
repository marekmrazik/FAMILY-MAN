ON hist.buyer_id = fm.user_id
WHERE A.DEMO_USER_ID = hist.buyer_id
AND hist.ebypls_buyer_status_cd in ( -1,1)
AND CURRENT_DATE - 2 BETWEEN hist.buyer_status_start_dt AND hist.buyer_status_end_dt
AND fm.tc = 'T'
);

