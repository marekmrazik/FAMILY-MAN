                        FROM access_views.dw_user_issue PII
						WHERE A.DEMO_USER_ID = PII.USER_ID AND
						scenario_id IN (1, 248, 250, 265, 491, 534, 535, 566, 629)
						AND status_id = 0		
						)
;

