			EXISTS	(
						SELECT *
						FROM PRS_SECURE_V.MDM_USER_PII AS PII
						WHERE 
							A.DEMO_USER_ID = PII.USER_ID AND
							PII.RGSTRD_YN = 'N'
						)
;

